{
  "name": "rx-reader",
  "version": "0.0.0",
  "description": "RSS Reader",
  "main": "index.js",
  "scripts": {
    "prestart": "npm rebuild node-sass",
    "start": "webpack-dev-server --inline --content-base dist/ --open --compress",
    "eslint": "./node_modules/.bin/eslint src/*.js",
    "accessibility": "node ./dev/accessibility-validation.js",
    "htmllint": "node ./dev/html-validator.js",
    "sasslint": "node ./node_modules/sass-lint/bin/sass-lint.js -v -q",
    "test": "npm run build && karma start && npm run eslint && npm run accessibility && npm run htmllint && npm run sasslint",
    "build": "rm -rf dist/ && webpack -p",
    "build:prod": "rm -rf dist/ && webpack -p --optimize-minimize",
    "sassdoc": "node ./dev/sassdoc.js",
    "docsserver": "npm run sassdoc && webpack-dev-server --port 8010 --config webpack.docs.config.js --content-base ./docs/ --open"
  },
  "keywords": [
    "RSS",
    "Reader",
    "Progressive Web App"
  ],
  "author": "Mr. Rampage",
  "license": "MIT",
  "devDependencies": {
    "access-sniff": "^3.0.1",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "eslint": "^4.4.1",
    "extract-loader": "^1.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "filehound": "^1.16.2",
    "html-loader": "^0.5.1",
    "html-validator": "^2.2.2",
    "jasmine-core": "^2.7.0",
    "karma": "^1.7.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.6",
    "sassdoc": "^2.3.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.2",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "rxjs": "^5.4.2",
    "whatwg-fetch": "^2.0.3",
    "xml-js": "^1.3.2"
  }
}
